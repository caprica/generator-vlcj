{
  "name": "generator-vlcj",
  "version": "1.0.2",
  "description": "Create a vlcj project that uses Maven",
  "homepage": "github.com/caprica/generator-vlcj",
  "author": {
    "name": "Mark Lee",
    "email": "mark.lee@capricasoftware.co.uk",
    "url": "http://capricasoftware.co.uk"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "vlcj",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-xo": "^0.29.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^29.3.1",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^7.0.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "mkdirp": "^1.0.4",
    "yeoman-generator": "^7.5.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
